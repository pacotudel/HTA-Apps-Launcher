<!DOCTYPE html>
<html>
<head>
<HTA:APPLICATION
  APPLICATIONNAME="Simple HTA Application Launcher"
  SYSMENU="yes">
<title>Simple HTA Application Launcher</title>
<link rel="stylesheet" href="css/cssbutton.css">

<style type="text/css">
body {background-color:lightsteelblue;}

.yui3-button {
    margin:3px;
}

.warning{
    background-color:#FAF55F;
}

.success{
    background-color:#57A957;
    color:white;
}

.error{
    background-color:#C43C35;
    color:white;
}

.notice{
    background-color:#1B7AE0;
    color:white;
}

.yui3-button-icon {
    background-image: url("media/icon-sprite-dark-and-light-24.png");
    background-repeat: no-repeat;
    display: inline-block;
    height: 20px;
    vertical-align: middle;
    width: 20px;
}

.yui3-button-icon-bold {
	background-position: 1px -1px;
}

.yui3-button-icon-italic {
	background-position: 1px -37px;
	background-image: url("media/icon-sprite-dark-and-light-24.png");
}

.yui3-button-icon-underline {
	background-position: 1px -73px;
	background-image: url("media/icon-sprite-dark-and-light-24.png");
}
			
.shadow{
    text-shadow: #333 6px -2px 2px;
}
</style>
<script language="javascript" type="text/javascript">
  window.resizeTo(640,580);
</script>

<script language="VBScript">
Sub RunRDP2TSFarm()
	Set objShell = CreateObject("WScript.Shell") 
	objShell.Run("mstsc /f /v:192.168.10.8"), 1, True
End Sub
Sub RunProgram()
	Set objShell = CreateObject("WScript.Shell") 
	objShell.Run("notepad.exe"), 1, True
End Sub
</script>
<meta http-equiv="x-ua-compatible" content="ie=9" />
</head>
<body>


<html>


<!--
